# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=medatiq_db
DB_USERNAME=root
DB_PASSWORD=your_password

# Server Configuration
PORT=4000
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
